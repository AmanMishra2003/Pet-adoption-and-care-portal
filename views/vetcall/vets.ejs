<%layout(' ../../layout/boilerplate.ejs') %>
    <style>
        .splide__slide img {
            width: 33%;
            height: 40vh;
            object-fit: cover;
            clip-path: polygon(11% 0, 100% 0%, 89% 100%, 0% 100%);
        }

        @media(max-width: 900px) {
            .splide__slide img {
                width: 100%;
                height: 40vh;
                object-fit: contain;
            }
            .vet-container {
                transform: skew(0) !important;
            }
            .row{
                transform: skew(0) !important;
            }
        }

        .clipPath {
            /* clip-path: polygon(0 0, 75% 0, 100% 100%, 25% 100%); */
            clip-path: polygon(0 0, 82% 0, 100% 100%, 19% 100%);
            /* transform: scale(1.5); */
        }

        .vet-container {
            background-color: rgb(233, 141, 50) !important;
            border-color: rgb(233, 141, 50) !important;
            color: white !important;

        }
    </style>

    <main>
        <section class="carousel row mb-5 border-bottom pb-5">
            <div class="col-12 text">
                <h1 class="text-center fw-bolder" style="color: rgb(233, 141, 50);">Our Vets</h1>
                <div class="row ">
                    <div class="col-12">
                        <section id="image-carousel" class="splide" aria-label="Beautiful Images">
                            <div class="splide__track">
                                <ul class="splide__list">
                                    <li class="splide__slide"  data-splide-interval="3000">
                                        <img src="data:image/jpeg;base64,"
                                            alt="">
                                        <img src="https://i.pinimg.com/474x/7f/b8/7b/7fb87b373cb67abc23018136d0c424b4.jpg"
                                            alt="">
                                        <img src="https://i.pinimg.com/474x/e4/f1/3f/e4f13f0255eb0a2aa35f154623c9054b.jpg"
                                            alt="">
                                    </li>
                                    <li class="splide__slide" data-splide-interval="3000">
                                        <img src="https://i.pinimg.com/474x/0b/56/fa/0b56fa0e894f41972d7105ec46a68abd.jpg"
                                            alt="">
                                        <img src="data:image/jpeg;base64,"
                                            alt="">
                                        <img src="https://i.pinimg.com/474x/14/36/3e/14363e61c3a304ef6a15fc8e757117b1.jpg"
                                            alt="">
                                    </li>
                                    <li class="splide__slide" data-splide-interval="3000">
                                        <img src="https://i.pinimg.com/474x/63/d2/16/63d216ac80300be8b705d05a608ad26a.jpg"
                                            alt="">
                                        <img src="https://i.pinimg.com/474x/b6/d9/d1/b6d9d1185caf98a4a75db2a92bdb0440.jpg"
                                            alt="">
                                        <img src="data:image/jpeg;base64,"
                                            alt="">
                                    </li>
                                </ul>
                            </div>

                        </section>
                    </div>
                </div>
            </div>
        </section>

        <section class="details container">
            <div class="row">
                <div class="col-12">
                    <div class="row">

                    </div>

                    <div class="row">
                        <div class="col-12 my-4">
                            <form action="/vet/vetDetails"
                                class="dog-image h-auto d-md-flex justify-content-around align-items-center"
                                style="border: 2px solid orange; " method="GET">
                                <div class="py-3 px-3 px-md-0">
                                    <a href="/vet/vetDetails" class="btn text-white"
                                        style="background-color:rgb(232, 126, 13) !important;">🐶 Clear all
                                        filter</a>
                                </div>
                                <div class="py-3 px-3 px-md-0"">
                                <label for=" petType" class="p-1">Search By State </label>
                                    <select class="form-select" aria-label="Default select example" name="state">
                                        <option <%=obj.state==="" ?"selected":"" %> value="">Select</option>
                                        <% for(let x of state) { %>
                                            <option value="<%= x %>" <%=obj.state===x?"selected":"" %>><%= x %>
                                            </option>
                                            <% } %>

                                    </select>
                                </div>
                                <div class="py-3 px-3">
                                    <label for="petType" class="p-1">Search By City </label>
                                    <select class="form-select" aria-label="Default select example" name="city">
                                        <option <%=obj.state==="" ?"selected":"" %> value="">Select</option>
                                        <% for(let y of cities) { %>
                                            <option value="<%= y %>" <%=obj.city===y?"selected":"" %>><%= y %>
                                            </option>
                                            <% } %>
                                    </select>
                                </div>
                                <div class="submit-button py-3 px-3 px-md-0 text-center">
                                    <button type="submit" class="btn text-white w-100"
                                        style="background-color:rgb(232, 126, 13) !important;">🐶 Submit</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="container-fluid " style="max-width: 90vw;">
        <div class="row justify-content-around w-100">
                <% for( let x of details ) { %>
                        <div class="col-md-5">
                            <div class="row justify-content-center mb-5">
                                <div class="col-md-4 p-0 clipPath bg-white ">
                                    <img src="<%= x.img.path %>" alt="" class="img-fluid"
                                        style="object-fit: cover; height: 100%; aspect-ratio: 3/2; object-position: center;">
                                </div>
                                <div class="col-md-7 mx-2 vet-container border" style="transform: skew(10deg);">
                                    <div class="row" style="transform: skew(-10deg);">
                                        <div class="col-12 px-5 py-5">
                                            <div class="border-bottom d-flex justify-content-between px-2 py-3">
                                                <p class="h3">
                                                    <%= x.clinicName %>
                                                </p>
                                                <p class="h3 ">
                                                    <%= x.name %>
                                                </p>
                                            </div>
                                            <div class="vet_details border-bottom mt-1">
                                                <p class="mb-1 text-dark">Specialisation : <span class="text-white"> <%= x.specialist %> </span></p>
                                                <p class="mb-1 text-dark">Qualifications : <span class="text-white"><%= x.qualification %></span></p>
                                                <p class=" text-dark">Experience : <span><%= x.exp %></span> Yrs</p>
                                            </div>
                                            <div class="mt-1">
                                                <p class="mb-1 text-dark">Clinic Address : <span class="text-white"><%= x.clinicAddress %></span></p>
                                                <p class="mb-1 text-dark">Operating Hour : <span class="text-white"><%=x.operatingHour.start%></span> -
                                                    <span class="text-white"><%=x.operatingHour.end%></span>
                                                </p>
                                            </div>
                                            <div class=" d-flex align-items-center mt-3">
                                                <a href="/vet/<%= x.id %>/appointment" class=" btn btn-success text-decoration-none">Book An
                                                    Appointment</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% } %>
                    </div>
            </div>

        </div>
    </section>


    </main>
    <script src="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js"></script>
    <script>
        new Splide('#image-carousel', {
            type: 'loop',
            autoplay: 'play',
            perPage: 1
        }).mount();
    </script>